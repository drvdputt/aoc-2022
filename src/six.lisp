(load "string.lisp")

(defvar example "mjqjpqmgbljsphdztnvjfqwrcgsmlb")
(defvar input "")

(defun first-unique-substring (s num)
  (loop for i from 0 to (- (length s) num)
	for end = (+ num i)
	when (not (has-duplicate-character (subseq s i end)))
	  return end))
  
(defun start-of-packet (s)
  (first-unique-substring s 4))

(defun start-of-message (s)
  (first-unique-substring s 14))

(print (start-of-packet input))
(print (start-of-message input))
